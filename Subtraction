.MODEL SMALL
.STACK 100H
.DATA
A DB "Enter the First Number: $"
B DB "Enter the Second Number: $"
C DB "Result: $"

.CODE
MAIN PROC
    MOV AX,@DATA         ; Setup data segment
    MOV DS,AX

    MOV AH,9            ; Print first prompt
    LEA DX,A            
    INT 21H

    MOV AH,1            ; Get first input
    INT 21H
    MOV BL,AL

    MOV AH,2            ; Print newline
    MOV DL,10
    INT 21H
    MOV DL,13           ; Print carriage return
    INT 21H

    MOV AH,9            ; Print second prompt
    LEA DX,B
    INT 21H

    MOV AH,1            ; Get second input
    INT 21H
    MOV BH,AL

    MOV AH,2            ; Print newline
    MOV DL,10
    INT 21H
    MOV DL,13           ; Print carriage return
    INT 21H

    MOV AH,9            ; Print result prompt
    LEA DX,C
    INT 21H

    SUB BL,BH           ; Subtract the numbers (this is different from the previous version)
    ADD BL,48           ; Convert to ASCII
    MOV DL,BL
    MOV AH,2
    INT 21H

    MOV AH,4CH          ; End program
    INT 21H

MAIN ENDP
END MAIN
